<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:Label text="userID" styleName="{style.important}" ui:field="labelUserID" />

		<g:VerticalPanel styleName="{style.important}"
			ui:field="userDataPanel">
			<g:Button text="aanpassen" styleName="{style.important}"
				ui:field="aanpassenKnop" />
		</g:VerticalPanel>

		<g:VerticalPanel styleName="{style.important}"
			ui:field="charPanel">
			<!-- <g:ScrollPanel addStyleNames='{style.important}' width="250px" height="600px" 
				ui:fiels="karakters"> -->
			<!-- <c:CellList ui:field="karaktersGegevens"> </c:CellList> -->
			<!-- </g:ScrollPanel> -->
			<g:Button text="verwijderen karakter" styleName="{style.important}"
				ui:field="verwijderCharKnop" />
			<g:Button text="nieuw karakter" styleName="{style.important}"
				ui:field="nieuwCharKnop" />
		</g:VerticalPanel>

		<g:VerticalPanel styleName="{style.important}"
			ui:field="adminPanel">
			<g:ListBox ui:field="locations" /> <!-- visibleItemCount='1' -->

		</g:VerticalPanel>

		<g:Button text="terug" styleName="{style.important}"
			ui:field="terugKnop" />

	</g:HTMLPanel>
</ui:UiBinder> 